{
    "contentDescriptors": {
        "null": {
            "name": "null",
            "schema": {
                "type": "null"
            }
        },
        "any": {
            "name": "any",
            "required": true,
            "schema": true
        },
        "integerResult": {
            "name": "result",
            "required": true,
            "schema": {
                "type": "integer"
            }
        },
        "authKey": {
            "name": "authKey",
            "description": "Authorization key.",
            "required": true,
            "schema": {
                "type": "string"
            }
        },
        "script": {
            "name": "script",
            "description": "Name of the script, MUST be unique among all registered scripts and MUST be a string consists of only letters, numbers and underlines.",
            "required": true,
            "schema": {
                "type": "string",
                "pattern": "^[A-Za-z0-9_]+$"
            }
        },
        "description": {
            "name": "description",
            "description": "Description of the script, SHOULD be any unicode string that can be displayed as plain text in Minecraft.",
            "required": true,
            "schema": {
                "type": "string"
            }
        },
        "permissionLevel": {
            "name": "permissionLevel",
            "description": "Permission level required to run the script.",
            "required": true,
            "schema": {
                "type": "integer",
                "minimum": 2,
                "maximum": 4
            }
        },
        "event": {
            "name": "event",
            "description": "Name of the event.",
            "required": true,
            "schema": {
                "type": "string"
            }
        },
        "callback": {
            "name": "callback",
            "description": "Name of the method called when event occurs.",
            "required": true,
            "schema": {
                "type": "string"
            }
        },
        "args": {
            "name": "args",
            "description": "Arguments to invoke the script.",
            "required": false,
            "schema": {
                "anyOf": [
                    {
                        "type": "object"
                    },
                    {
                        "type": "array"
                    }
                ]
            }
        },
        "interface": {
            "name": "interface",
            "description": "Name of the interface, MUST be unique among all interfaces and MUST be a string consists of only letters, numbers and underlines.",
            "required": true,
            "schema": {
                "type": "string",
                "pattern": "^[A-Za-z0-9_]+$"
            }
        },
        "data": {
            "name": "data",
            "description": "Data to be read or written in an little-endian array of 32 bit integers.",
            "required": true,
            "schema": {
                "type": "array",
                "items": {
                    "type": "integer"
                }
            }
        },
        "message": {
            "name": "message",
            "description": "Message to be logged, SHOULD be any unicode string that can be displayed as plain text in Minecraft.",
            "required": true,
            "schema": {
                "type": "string"
            }
        },
        "uuid": {
            "name": "uuid",
            "description": "UUID of the player.",
            "required": true,
            "schema": {
                "type": "string"
            }
        }
    },
    "errors": {
        "InvalidAuthKey": {
            "code": -1,
            "message": "Invalid authorization key."
        },
        "IllegalName": {
            "code": -2,
            "message": "Illegal name."
        },
        "NameExists": {
            "code": -3,
            "message": "Target with the name already exists."
        },
        "InvalidPermissionLevel": {
            "code": -4,
            "message": "Invalid permission level."
        },
        "ScriptNotRegistered": {
            "code": -5,
            "message": "Script is not registered."
        },
        "IllegalArgument": {
            "code": -6,
            "message": "Illegal argument."
        },
        "ScriptInternalError": {
            "code": -7,
            "message": "Script internal error."
        },
        "EventNotFound": {
            "code": -8,
            "message": "Event is not found."
        },
        "EventCallbackAlreadyRegistered": {
            "code": -9,
            "message": "Event callback is already registered."
        },
        "EventCallbackNotRegistered": {
            "code": -10,
            "message": "Event callback is not registered."
        },
        "AccessDenied": {
            "code": -11,
            "message": "Access denied."
        },
        "InterfaceNotFound": {
            "code": -12,
            "message": "Interface is not found."
        }
    }
}